<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:screenOrientation="portrait"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.rama.arogya.EmotionsActivity"
    android:id="@+id/ABCCC">

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="What Do You Feel"
        android:id="@+id/textView_feel"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:gravity="center_horizontal" />

    <ImageView
        android:layout_width="200px"
        android:layout_height="200px"
        android:id="@+id/imageView_happy"
        android:layout_below="@+id/textView_feel"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="28dp"
        android:layout_marginLeft="30dp"
        android:src="@drawable/happy" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Joy"
        android:id="@+id/textView_joy"
        android:layout_below="@+id/imageView_happy"
        android:layout_alignLeft="@+id/imageView_happy"
        android:layout_alignStart="@+id/imageView_happy"
        android:layout_marginLeft="20dp" />

    <ImageView
        android:layout_width="200px"
        android:layout_height="200px"
        android:id="@+id/imageView_love"
        android:src="@drawable/love"
        android:layout_marginRight="46dp"
        android:layout_marginEnd="46dp"
        android:layout_above="@+id/textView_joy"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Love"
        android:id="@+id/textView2"
        android:layout_below="@+id/imageView_love"
        android:layout_alignLeft="@+id/imageView_love"
        android:layout_alignStart="@+id/imageView_love"
        android:layout_marginLeft="35dp" />

    <ImageView
        android:layout_width="200px"
        android:layout_height="200px"
        android:id="@+id/imageView_Sad"
        android:src="@drawable/tired"
        android:layout_alignTop="@+id/imageView_Surprise"
        android:layout_alignLeft="@+id/imageView_happy"
        android:layout_alignStart="@+id/imageView_happy" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Sad"
        android:id="@+id/textView_tired"
        android:layout_below="@+id/imageView_Sad"
        android:layout_alignLeft="@+id/imageView_Sad"
        android:layout_alignStart="@+id/imageView_Sad"
        android:layout_marginLeft="25dp" />

    <ImageView
        android:layout_width="200px"
        android:layout_height="200px"
        android:id="@+id/imageView_Surprise"
        android:src="@drawable/surprise"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/imageView_love"
        android:layout_alignStart="@+id/imageView_love" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Surprise"
        android:id="@+id/textView_relieved"
        android:layout_below="@+id/imageView_Surprise"
        android:layout_alignLeft="@+id/imageView_Surprise"
        android:layout_alignStart="@+id/imageView_Surprise"
        android:layout_marginLeft="25dp" />

    <ImageView
        android:layout_width="200px"
        android:layout_height="200px"
        android:id="@+id/imageView_angry"
        android:layout_marginTop="28dp"
        android:src="@drawable/angry"
        android:layout_below="@+id/textView_tired"
        android:layout_alignLeft="@+id/imageView_Sad"
        android:layout_alignStart="@+id/imageView_Sad" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Anger"
        android:id="@+id/textView_angry"
        android:layout_below="@+id/imageView_angry"
        android:layout_alignLeft="@+id/imageView_angry"
        android:layout_alignStart="@+id/imageView_angry"
        android:layout_marginLeft="25dp" />

    <ImageView
        android:layout_width="200px"
        android:layout_height="200px"
        android:id="@+id/imageView_fear"
        android:src="@drawable/fearful"
        android:layout_alignTop="@+id/imageView_angry"
        android:layout_alignLeft="@+id/imageView_Surprise"
        android:layout_alignStart="@+id/imageView_Surprise" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Anxiety"
        android:id="@+id/textView_fearful"
        android:layout_alignBottom="@+id/textView_angry"
        android:layout_alignLeft="@+id/imageView_fear"
        android:layout_alignStart="@+id/imageView_fear"
        android:layout_marginLeft="28dp" />

</RelativeLayout>
